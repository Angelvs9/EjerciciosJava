<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="820.0" prefWidth="1432.0" styleClass="fondo" stylesheets="styles/estilo.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xmlControllers.ControllerLogin1">
    <children>
        <Button fx:id="botonInicioSesion" layoutX="596.0" layoutY="640.0" onAction="#accionBotonInicioSesion1" prefHeight="50.0" prefWidth="240.0" text="ACEPTAR" textAlignment="CENTER">
        </Button>
        <Label fx:id="labelNo" contentDisplay="CENTER" layoutX="528.0" layoutY="734.0" minHeight="16" minWidth="69" prefHeight="45.0" prefWidth="376.0" text="      El usuario no es correcto o no existe" visible="false">
         <font>
            <Font size="18.0" />
         </font></Label>
      <PasswordField fx:id="inputPassword" layoutX="511.0" layoutY="460.0" prefHeight="80.0" prefWidth="410.0" promptText="CONTRASEÑA" styleClass="inputPassword" stylesheets="styles/estilo.css">
         <font>
            <Font size="27.0" />
         </font>
      </PasswordField>
      <TextField fx:id="inputUsuario" layoutX="511.0" layoutY="340.0" prefHeight="80.0" prefWidth="410.0" promptText="USUARIO" styleClass="inputTexto" stylesheets="styles/estilo.css">
         <font>
            <Font size="27.0" />
         </font></TextField>
      <ImageView fx:id="loginImg" fitHeight="221.0" fitWidth="321.0" layoutX="596.0" layoutY="97.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/log.png" />
         </image>
      </ImageView>
      <Button fx:id="botonAnterior2" layoutX="44.0" layoutY="43.0" mnemonicParsing="false" onAction="#goToScreenAnterior2" text="&lt;">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="botonIrRegistro" layoutX="946.0" layoutY="622.0" onAction="#goToScreenRegistro" prefHeight="50.0" prefWidth="338.0" text="¿No estas registrado?" textAlignment="CENTER" />
    </children>
</AnchorPane>
